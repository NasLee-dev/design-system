{
  "extends": "nx/presets/npm.json",
	"affected": {
		"defaultBase": "main"
	},
	"workspaceLayout": {
		"libsDir": "packages",
		"appsDir": "services"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build"]
			}
		}
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	}
}